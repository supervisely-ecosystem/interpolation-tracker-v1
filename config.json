{
    "name": "Interpolation tracker",
    "type": "app",
    "categories": [
      "tracking",
      "video"
    ],
    "description": "Track polygons, rectangles and points using linear interpolation",
    "docker_image": "supervisely/base-py-sdk:6.71.8",
    "main_script": "src/main.py",
    "modal_template": "src/modal.html",
    "modal_template_state": {
      "shapeComplexity": "greedily"
    },
    "task_location": "application_sessions",
    "icon": "https://img.icons8.com/color/100/000000/5.png",
    "icon_cover": true,
    "isolate": true,
    "headless": true,
    "session_tags": [
      "sly_video_tracking"
    ],
    "allowed_shapes" : [
      "polygon",
      "point",
      "rectangle"
    ],
    "community_agent": true
  }